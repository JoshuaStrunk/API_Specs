{
  "name": "Matchmaker",
  "url": "/Matchmaker",
  "description": "Use an external Match Making service to handle finding a server for a new player to be dropped into.",
  "datatypes": {
    "AuthUserRequest": {
      "name": "AuthUserRequest",
      "properties": [
        {
          "name": "AuthorizationTicket",
          "description": "Ticket passed in by user that allows the user to join a game",
          "jsontype": "String",
          "actualtype": "String",
          "optional": false
        }
      ]
    },
    "AuthUserResponse": {
      "name": "AuthUserResponse",
      "properties": [
        {
          "name": "Authorized",
          "description": "boolean indicating if the user has been authorised to use the external matchmaker API or not",
          "jsontype": "Boolean",
          "actualtype": "Boolean",
          "optional": false
        },
        {
          "name": "PlayFabId",
          "description": "unique user id of the account that has been authorised by the game server instance",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        }
      ]
    },
    "UserInfoRequest": {
      "name": "UserInfoRequest",
      "properties": [
        {
          "name": "PlayFabId",
          "description": "Unique account PlayFabId that user info is being asked for for",
          "jsontype": "String",
          "actualtype": "String",
          "optional": false
        },
        {
          "name": "MinCatalogVersion",
          "description": "Minimum catalog version to return data for - e.g. if we are at catalog version 5, but there are still items from catalog version 3 in the users inventory, setting this to 4 will mean those version 3 items will be filtered out.",
          "jsontype": "Number",
          "actualtype": "uint32",
          "optional": false
        }
      ]
    },
    "UserInfoResponse": {
      "name": "UserInfoResponse",
      "properties": [
        {
          "name": "PlayFabId",
          "description": "Unique account UserID that user info is being asked for for",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "Username",
          "description": "account name",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "DisplayName",
          "description": "actual account display name",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "TitleDisplayName",
          "description": "actual game specific display name, if set",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "Inventory",
          "description": "array of users inventory for this game",
          "collection": "array",
          "jsontype": "Object",
          "actualtype": "ItemInstance",
          "isclass": true,
          "optional": true
        },
        {
          "name": "VirtualCurrency",
          "description": "array of amounts of virtual currency the player currently has",
          "collection": "map",
          "jsontype": "Number",
          "actualtype": "int32",
          "optional": true
        },
        {
          "name": "IsDeveloper",
          "description": "marker that describes if the user is a developer or not. Set by CSR tools, not game related APIs's",
          "jsontype": "Boolean",
          "actualtype": "Boolean",
          "optional": false
        },
        {
          "name": "SteamId",
          "description": "Steam ID if the user is associated with a steam account",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        }
      ]
    },
    "ItemInstance": {
      "name": "ItemInstance",
      "properties": [
        {
          "name": "ItemId",
          "description": "Object name",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "ItemInstanceId",
          "description": "unique item id",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "ItemClass",
          "description": "class name object belongs to",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "PurchaseDate",
          "description": "date purchased",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "Expiration",
          "description": "date object will expire (optional)",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "RemainingUses",
          "description": "number of remaining uses (optional)",
          "jsontype": "Number",
          "actualtype": "uint32",
          "optional": true
        },
        {
          "name": "Annotation",
          "description": "game specific comment",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "CatalogVersion",
          "description": "catalog version that this item is part of",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "BundleParent",
          "description": "Unique ID of the parent of where this item may have come from (e.g. if it comes from a crate or coupon)",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        }
      ]
    },
    "StartGameRequest": {
      "name": "StartGameRequest",
      "properties": [
        {
          "name": "Build",
          "description": "which uploaded build of the game server we are starting up",
          "jsontype": "String",
          "actualtype": "String",
          "optional": false
        },
        {
          "name": "Region",
          "description": "the region we want to associate this server with for filtering servers",
          "jsontype": "String",
          "actualtype": "Region",
          "isenum": true,
          "optional": false
        },
        {
          "name": "GameMode",
          "description": "which user defined game mode this server is going to be running (e.g. Capture The Flag = 0, Deathmatch = 1) - default to 0 if there is only one mode",
          "jsontype": "Number",
          "actualtype": "uint32",
          "optional": false
        },
        {
          "name": "Subscriber",
          "description": "External matchmaker server to post status updates about the game",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        }
      ]
    },
    "Region": {
      "name": "Region",
      "isenum": true,
      "enumvalues": [
        {
          "name": "USWest"
        },
        {
          "name": "USCentral"
        },
        {
          "name": "USEast"
        },
        {
          "name": "EUWest"
        },
        {
          "name": "APSouthEast"
        },
        {
          "name": "APNorthEast"
        },
        {
          "name": "SAEast"
        },
        {
          "name": "Australia"
        },
        {
          "name": "China"
        },
        {
          "name": "UberLan"
        }
      ]
    },
    "StartGameResponse": {
      "name": "StartGameResponse",
      "properties": [
        {
          "name": "LobbyID",
          "description": "unique ID for the lobby of that servers the new game",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "Region",
          "description": "the region we want to associate this server with for filtering servers",
          "jsontype": "String",
          "actualtype": "Region",
          "isenum": true,
          "optional": true
        },
        {
          "name": "GameMode",
          "description": "which user defined game mode this server is going to be running (e.g. Capture The Flag = 0, Deathmatch = 1) - default to 0 if there is only one mode",
          "jsontype": "Number",
          "actualtype": "uint32",
          "optional": false
        },
        {
          "name": "Build",
          "description": "which uploaded build of the game server we are starting up",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "Address",
          "description": "IP address the server is located at",
          "jsontype": "String",
          "actualtype": "String",
          "optional": true
        },
        {
          "name": "Port",
          "description": "port number to communicate with the server on",
          "jsontype": "Number",
          "actualtype": "uint32",
          "optional": false
        }
      ]
    },
    "PlayerJoinedRequest": {
      "name": "PlayerJoinedRequest",
      "properties": [
        {
          "name": "ServerId",
          "description": "Server ID of external match maker we are using for match making",
          "jsontype": "String",
          "actualtype": "String",
          "optional": false
        },
        {
          "name": "PlayFabId",
          "description": "Unique account UserID that user info is being asked for for",
          "jsontype": "String",
          "actualtype": "String",
          "optional": false
        }
      ]
    },
    "PlayerJoinedResponse": {
      "name": "PlayerJoinedResponse",
      "properties": []
    },
    "PlayerLeftRequest": {
      "name": "PlayerLeftRequest",
      "properties": [
        {
          "name": "ServerId",
          "description": "Server ID of external match maker we are using for match making",
          "jsontype": "String",
          "actualtype": "String",
          "optional": false
        },
        {
          "name": "PlayFabId",
          "description": "Unique ID of player that has left the game",
          "jsontype": "String",
          "actualtype": "String",
          "optional": false
        }
      ]
    },
    "PlayerLeftResponse": {
      "name": "PlayerLeftResponse",
      "properties": []
    }
  },
  "calls": [
    {
      "name": "AuthUser",
      "summary": "Authorize a particular user. This allows the external match making process a way to tell the playfab system which Game Server Instance a player can be added to.",
      "requestDetails": "This API is what the external match making server would call to inform the Playfab system which server a player has been match maked into.",
      "subgroup": "Matchmaking APIs",
      "url": "/Matchmaker/AuthUser",
      "auth": "SecretKey",
      "method": "POST",
      "request": "AuthUserRequest",
      "result": "AuthUserResponse",
      "errors": [
        "InvalidParams"
      ]
    },
    {
      "name": "PlayerJoined",
      "summary": "External service calls into the Playfab System to indicate that, according to the external service, a player has joined a specific server.",
      "subgroup": "Matchmaking APIs",
      "url": "/Matchmaker/PlayerJoined",
      "auth": "SecretKey",
      "method": "POST",
      "request": "PlayerJoinedRequest",
      "result": "PlayerJoinedResponse",
      "errors": [
        "InvalidParams"
      ]
    },
    {
      "name": "PlayerLeft",
      "summary": "External service calls into the Playfab System to indicate that, according to the external service,  a player has left a specific server.",
      "subgroup": "Matchmaking APIs",
      "url": "/Matchmaker/PlayerLeft",
      "auth": "SecretKey",
      "method": "POST",
      "request": "PlayerLeftRequest",
      "result": "PlayerLeftResponse",
      "errors": [
        "InvalidParams"
      ]
    },
    {
      "name": "StartGame",
      "summary": "Allow an external match making service to start a new Game Server Instance, if it has decided it needs to.",
      "subgroup": "Matchmaking APIs",
      "url": "/Matchmaker/StartGame",
      "auth": "SecretKey",
      "method": "POST",
      "request": "StartGameRequest",
      "result": "StartGameResponse",
      "errors": [
        "InvalidParams"
      ]
    },
    {
      "name": "UserInfo",
      "summary": "Get UserInfo for a specific user, so the external system can have data on hand to match make against",
      "requestDetails": "This allows the external system to ask for specific details on players in a given match, in order to know if it's appropriate to match make a new player against them.",
      "subgroup": "Matchmaking APIs",
      "url": "/Matchmaker/UserInfo",
      "auth": "SecretKey",
      "method": "POST",
      "request": "UserInfoRequest",
      "result": "UserInfoResponse",
      "errors": [
        "InvalidParams"
      ]
    }
  ],
  "subgroups": [
    {
      "name": "Matchmaking APIs",
      "methods": [
        "AuthUser",
        "PlayerJoined",
        "PlayerLeft",
        "StartGame",
        "UserInfo"
      ]
    }
  ],
